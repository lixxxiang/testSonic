<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:app="http://schemas.android.com/apk/res-auto"
                xmlns:tools="http://schemas.android.com/tools"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                tools:context=".MainActivity">

    <com.github.lzyzsd.jsbridge.BridgeWebView android:layout_width="match_parent" android:layout_height="match_parent"
                                              android:id="@+id/webView"/>
    <Button android:layout_alignParentBottom="true" android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/btn"/>
    <Button android:layout_alignParentBottom="true" android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@+id/btn" android:id="@+id/btn2"/>

</RelativeLayout>
